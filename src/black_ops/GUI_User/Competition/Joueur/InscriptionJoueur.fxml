                        <Insets left="30.0" right="30.0" />
                     </HBox.margin>
                  </ComboBox>
                  <ComboBox fx:id="ComboCommunaute" onAction="#showComu" prefWidth="150.0" promptText="Commuanté" style="-fx-border-color: #09a195; -fx-background-color: transparent;">
