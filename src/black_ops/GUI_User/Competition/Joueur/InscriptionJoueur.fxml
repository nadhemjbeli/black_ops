<AnchorPane id="AnchorPane" prefHeight="900.0" prefWidth="1800.0" style="-fx-background-color: #0f1316;" styleClass="mainFxmlClass" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="black_ops.GUI_User.Competition.Joueur.InscriptionJoueurController">
